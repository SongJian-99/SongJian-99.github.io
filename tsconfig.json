{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "types": ["node"],
    "skipLibCheck": true
  },
  "include": [
    "src/.vuepress/**/*.ts",
    "src/.vuepress/**/*.vue",
    "src/.vuepress/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
